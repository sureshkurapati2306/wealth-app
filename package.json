{
  "name": "cimb",
  "version": "0.9.13",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "prepare": "husky install",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "SSP": "ng serve -o",
    "SSP:test": "ng serve --configuration=test -o",
    "SSP:uat": "ng serve --configuration=uat -o",
    "build": "ng build --subresource-integrity",
    "build:dev": "ng build --configuration=development",
    "build:test": "ng build --configuration=test",
    "build:uat": "ng build --configuration=uat --subresource-integrity",
    "build:prod": "npm run affected:build --prod --base-href=./",
    "BO": "ng serve wealth-backoffice --port=4300 -o",
    "BO:test": "ng serve wealth-backoffice --port=4300 --configuration=test -o",
    "BO:uat": "ng serve wealth-backoffice --port=4300 --configuration=uat -o",
    "BO build:dev": "ng build wealth-backoffice --configuration=development",
    "BO build:test": "ng build wealth-backoffice --configuration=test",
    "BO build:uat": "ng build wealth-backoffice --configuration=uat --subresource-integrity",
    "BO build:prod": "npm run affected:build wealth-backoffice --prod --base-href=./",
    "ci": "npm run affected:lint && npm run affected:test && npm run build:prod",
    "test": "ng test --codeCoverage=true",
    "test:self-serve": "nx test self-serve --codeCoverage --silent=true",
    "test:ci": "ng test --watch=false --browsers=Headless --source-map=false --codeCoverage=true",
    "test:ci:watch": "ng test --watch=true --source-map=false --codeCoverage=true",
    "test:all": "nx run-many --all --target=test --codeCoverage  --detectOpenHandles --only-failed",
    "lint": "nx workspace-lint && ng lint --detectOpenHandles --only-failed",
    "lint:all": "nx run-many --all --target=lint --detectOpenHandles --only-failed",
    "e2e": "ng e2e",
    "affected:apps": "nx affected --target=apps --base=origin/release-0.9",
    "affected:libs": "nx affected --target=libs --base=origin/release-0.9",
    "affected:build": "nx affected --target=build --base=origin/release-0.9",
    "affected:e2e": "nx affected --target=e2e --base=origin/release-0.9",
    "affected:test": "nx affected --target=test --parallel=5 --codeCoverage --base=origin/release-0.9",
    "affected:lint": "nx affected --target=lint --parallel=5 --base=origin/release-0.9",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "doctor": "nx run self-serve:test --codeCoverage && nx run-many --all --target=lint && nx run-many --all --target=test --codeCoverage --only-failed"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/environments/",
      "model",
      "environments"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/environments/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/environments/",
      "model",
      "environments"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/environments/",
      "model",
      "(.test)\\.(ts|tsx|js)$",
      "/distribution/.*\\.(ts|js)$"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/datetime-picker": "^7.0.1",
    "@angular/animations": "12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "12.2.16",
    "@angular/compiler": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "12.2.16",
    "@angular/material": "12.2.13",
    "@angular/material-moment-adapter": "^12.2.13",
    "@angular/platform-browser": "12.2.16",
    "@angular/platform-browser-dynamic": "12.2.16",
    "@angular/router": "12.2.16",
    "@ng-idle/core": "^11.1.0",
    "@ng-idle/keepalive": "^11.0.3",
    "@ngrx/component-store": "12.5.1",
    "@ngrx/effects": "12.5.1",
    "@ngrx/entity": "12.5.1",
    "@ngrx/router-store": "12.5.1",
    "@ngrx/store": "12.5.1",
    "@nrwl/angular": "12.10.1",
    "@types/node-forge": "^1.3.1",
    "angular-google-charts": "^2.2.2",
    "file-saver": "^1.3.2",
    "highcharts": "^9.3.3",
    "highcharts-angular": "^2.10.0",
    "jwt-decode": "^3.1.2",
    "lodash-es": "^4.17.21",
    "modern-normalize": "^1.1.0",
    "moment": "^2.29.4",
    "ngx-device-detector": "^2.2.1",
    "ngx-pagination": "^5.1.1",
    "node-forge": "^1.3.1",
    "pdfmake": "^0.2.7",
    "rxjs": "~6.6.0",
    "swiper": "^6.8.4",
    "tslib": "^2.0.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.18",
    "@angular-devkit/schematics": "^12.2.18",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "12.2.18",
    "@angular/compiler-cli": "12.2.16",
    "@angular/language-service": "12.2.16",
    "@ngrx/schematics": "12.5.1",
    "@ngrx/store-devtools": "12.5.1",
    "@nrwl/cli": "12.10.1",
    "@nrwl/cypress": "12.10.1",
    "@nrwl/eslint-plugin-nx": "12.10.1",
    "@nrwl/jest": "12.10.1",
    "@nrwl/linter": "12.10.1",
    "@nrwl/tao": "12.10.1",
    "@nrwl/workspace": "12.10.1",
    "@types/jest": "26.0.8",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "14.14.33",
    "@types/pdfmake": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "cypress": "7.4.0",
    "dotenv": "10.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^7.0.0",
    "jasmine-marbles": "^0.9.2",
    "jest": "27.0.3",
    "jest-preset-angular": "9.0.7",
    "lint-staged": "^11.2.3",
    "prettier": "2.4.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "typescript": "4.3.5"
  }
}
